-dim 2   				  # cell dimension (= space dimension)
-dt 1e-2         # time step
-renumber_dofs 0  # dofs renumbering
-maxts 500       # maximum number of time steps
-print_step 3     # print step
-has_convec 1     # convective term: true or false
-unsteady   1     # steady or unsteady problem
-beta1 0        # Vmesh = bet1 * Ufluid  +  bet2 * Uelastic
-beta2 1
plot_es 1         # plot exact solution: true or false
-family_files 1   # plot output as 001, 002, 003, ...
-sst 1.e-8        # steady state tol (Unew-Uold)/(Unew)
-theta 1.0        # time discretization theta method
-quadr_c 4        # quadrature degree (cell)
-quadr_f 3        # quadrature degree (facet)
-quadr_r 2        # quadrature degree (facet)
-quadr_e 8        # quadrature degree (facet)
-ale 1

# Petsc options
-pc_type asm                        # Preconditioner (one of) none jacobi pbjacobi bjacobi sor lu shell mg eisenstat ilu icc cholesky asm ksp composite redundant nn mat fieldsplit galerkin exotic openmp asa cp bfbt lsc redistribute tfs (PCSetType)
pc_composite_type multiplicative   # one of multiplicative, additive, special
pc_composite_pcs ilu,ilu
-ksp_type bcgsl gmres                  # (one of) cg cgne nash stcg gltr richardson chebychev gmres tcqmr bcgs ibcgs bcgsl cgs tfqmr cr lsqr preonly qcg bicg fgmres minres symmlq lgmres lcd broyden gcr (KSPSetType)
-ksp_initial_guess_nonzero 1
pc_factor_shift_type NONZERO
pc_factor_shift_amount 1.e-10
-pc_factor_mat_ordering_type nd    # natural nd 1wd rcm qmd rowlength flow (PCFactorSetMatOrderingType)
pc_factor_nonzeros_along_diagonal 1.e-10
pc_factor_diagonal_fill
-pc_factor_reuse_ordering 1
pc_factor_fill 3.22746e-06
pc_factor_in_place
ksp_rtol 1e-11

vec_type seqpthread
mat_type seqaijpthread

fp_trap 1           # stop on floating-point exceptions
-on_error_abort 1
log_trace stdout
malloc_debug 1
-snes_fd 0
-snes_ls basic      # line search: basic, cubic, quadratic
-snes_type ksponly   # Nonlinear solver method (one of) ls tr test picard ksponly vi ngmres sorqn
snes_test_display 1 # compare f.e. jacobian with f.d. jacobian
snes_monitor_cancel 1 # cancela monitoramento
ksp_monitor stdout
-snes_max_it 5
mat_no_inode 1


# DEBUG
-print_to_matlab 0  # prints jacobian to matlab
-force_dirichlet 1  # forces dirichlet b.c.


##################### Boundary conditions (dir; neu) #################################
#  (dir, neumman, interface)
# cavity problem: (1,2; 0)
# kovasznay problem: (2; 1)
# satic bubble (0;0;2)
# osc bubble (0;0;2)
# stokes 2d (jean-donea) (1,2; 0)
# anything 3d (2;1)
# sloshing (5;0;2;3,4;20,21)
# ramp ( ; ;2;3;20,21)
dir_tags 5
neum_tags 1
-interf_tags 2
-solid_tags 3
-triple_tags 20,21


############################################################################

function_space 1 # P1P1 (or Q1Q1) GLS stabilization
function_space 2 # P1+P1 with bubble condensation
function_space 3 # P2+Pm1 with bubble condensation and pressure gradient elimination
-function_space 4 # P2P1 (or Q2Q1)
function_space 5 # P1+P1
function_space 6 # P2P0
function_space 7 # P2+Pm1 full                                                       

mesh_type 4      # TRIANGLE3        = 0x4,
-mesh_type 8      # TRIANGLE6        = 0x8,
mesh_type 16     # QUADRANGLE4      = 0x10,
mesh_type 32     # QUADRANGLE8      = 0x20,
mesh_type 64     # QUADRANGLE9      = 0x40,
mesh_type 128    # TETRAHEDRON4     = 0x80,
mesh_type 256    # TETRAHEDRON10    = 0x100,
mesh_type 512    # HEXAHEDRON8      = 0x200,
mesh_type 1024   # HEXAHEDRON20     = 0x400,
mesh_type 2048   # HEXAHEDRON27     = 0x800,


