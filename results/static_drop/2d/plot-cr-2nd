set key right top #legend position
set logscale
plot 'errors-cr-2nd' u ($1):($2) w lp title 'velocity', \
     'errors-cr-2nd' u ($1):($3) w lp title 'pressure', \
     'errors-cr-2nd' u ($1):($4) w lp title 'velocity grad', \
0.005*x**3 title 'x^3', \
0.0005*x**2 lt 17 title 'x^2', \
0.002*x lt 9 title 'x'

