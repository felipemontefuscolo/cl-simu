set key right top #legend position
set logscale
plot 'errors-gls-1st' u ($1):($2) w lp title 'velocity', \
     'errors-gls-1st' u ($1):($3) w lp title 'pressure', \
     'errors-gls-1st' u ($1):($4) w lp title 'velocity grad', \
     'errors-gls-1st' u ($1):($5) w lp title 'pressure grad', \
0.25*x**2 title 'x^2', \
0.000002*x lt 9 title 'x'

