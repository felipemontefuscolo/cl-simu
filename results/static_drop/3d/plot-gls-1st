     'e-gls-1st' u ($1):($3) w lp title 'pressure', \
     'e-gls-1st' u ($1):($4) w lp title 'velocity grad', \
     'e-gls-1st' u ($1):($5) w lp title 'pressure grad', \
1*x lt 9 title 'x' ,\
0.1*x**(1.7) lt 9 title 'x^2'

