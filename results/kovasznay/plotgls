set key left top #legend position
set logscale
plot 'errorsgls' u ($1):($2) w lp title 'velocity', \
     'errorsgls' u ($1):($3) w lp title 'pressure', \
     'errorsgls' u ($1):($4) w lp title 'velocity grad', \
     'errorsgls' u ($1):($5) w lp title 'pressure grad', \
0.35*x**2 title 'x^2', \
2.8*x lt 9 title 'x'

