set key left top #legend position
set logscale
plot 'errorscrm' u ($1):($2) w lp title 'velocity', \
     'errorscrm' u ($1):($3) w lp title 'pressure', \
     'errorscrm' u ($1):($4) w lp title 'velocity grad', \
     'errorscrm' u ($1):($5) w lp title 'pressure grad', \
0.07*x**3 title 'x^3', \
1.2*x**2 lt 17 title 'x^2', \
0.15*x lt 17 title 'x'

