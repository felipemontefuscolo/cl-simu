set key left top #legend position
set logscale
plot 'errors-cr' u ($1):($2) w lp title 'velocity', \
     'errors-cr' u ($1):($3) w lp title 'pressure', \
     'errors-cr' u ($1):($4) w lp title 'velocity grad', \
0.07*x**3 title 'x^3', \
0.001*x**2 title 'x^2', \
0.09*x lt 9 title 'x'

